  "dependencies": {
    "d3-array": "^2.0.3",
    "d3-fetch": "^1.1.2",
    "d3-scale-chromatic": "^3.0.0",
    "d3-time": "^1.0.11",
    "d3-time-format": "^2.1.3",
    "leaflet": "^1.4.0",
    "react": "^16.8.2",
